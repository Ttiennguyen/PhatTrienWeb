@model List<WebChild.Data.Category>

@foreach (var item in Model)
{
    @if (@item.CategoryName!="Child Seats")
    {
        <li class="menu_item down"><a asp-controller="Product" asp-action="List" asp-route-id="@item.id">@item.CategoryName</a></li>

    }
    @if (@item.CategoryName == "Child Seats")
    {
        <li class="last_item menu_item down"><a asp-controller="Product" asp-action="List" asp-route-id="@item.id">@item.CategoryName</a></li>

    }
    
}
